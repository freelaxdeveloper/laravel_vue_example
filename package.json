{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "npm run config:local && cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "npm run config:prod && cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && npm run config:dev",
        "build:dev": "npm run config:dev && cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && npm run config:local",
        "config:local": "cp resources/assets/js/App/config/config.local.js resources/assets/js/App/config.js",
        "config:dev": "cp resources/assets/js/App/config/config.dev.js resources/assets/js/App/config.js",
        "config:prod": "cp resources/assets/js/App/config/config.prod.js resources/assets/js/App/config.js"
    },
    "devDependencies": {
        "axios": "^0.19.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "bootstrap": "^4.3.1",
        "cross-env": "^5.1",
        "howler": "^2.0.15",
        "jquery": "^3.4.1",
        "laravel-mix": "^2.0",
        "lodash": "^4.17.4",
        "popper.js": "^1.12",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "vue": "^2.6.2",
        "vue-template-compiler": "^2.6.2"
    },
    "dependencies": {
        "@mdi/font": "^2.8.94",
        "@minogin/vue-drag-resize-rotate": "^1.0.5",
        "begeta": "^1.0.0",
        "buefy": "^0.7.2",
        "cxlt-vue2-toastr": "^1.1.0",
        "echarts": "^4.2.0-rc.2",
        "express": "^4.16.3",
        "jade": "^1.11.0",
        "material-design-icons-iconfont": "^3.0.3",
        "pug": "^3.0.1",
        "v-animate-css": "0.0.3",
        "vue-clipboard2": "^0.2.1",
